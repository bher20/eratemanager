{
  "id": null,
  "uid": "eratemanager-main",
  "title": "eRateManager Overview",
  "schemaVersion": 36,
  "version": 1,
  "tags": [
    "eratemanager",
    "energy",
    "rates"
  ],
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests per provider (rate)",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (provider) (rate(eratemanager_requests_total[5m]))",
          "legendFormat": "{{provider}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request latency p95 per provider",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le,provider) (rate(eratemanager_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95 {{provider}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors per provider and code",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (provider,code) (rate(eratemanager_request_errors_total[5m]))",
          "legendFormat": "{{provider}} ({{code}})"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        }
      }
    ]
  },
  "time": {
    "from": "now-24h",
    "to": "now"
  }
}